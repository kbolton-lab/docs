{
  "name": "vardict_multithreaded",
  "title": "Vardict Java multithreaded",
  "summary": "Run Vardict Java and bcbio filter",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "inputSpec": [
    {
      "name": "reference",
      "label": "reference assembly fasta",
      "class": "file",
      "optional": true,
      "patterns": [
        "*.fa",
        "*.fasta"
      ],
      "help": ""
    },
    {
      "name": "reference_index",
      "label": "reference assembly index",
      "class": "file",
      "optional": true,
      "patterns": [
        "*.fai"
      ],
      "help": ""
    },
    {
      "name": "reference_dict",
      "label": "reference assembly dict",
      "class": "file",
      "optional": true,
      "patterns": [
        "*.dict"
      ],
      "help": ""
    },
    {
      "name": "tumor_bam",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.bam"
      ],
      "help": ""
    },
    {
      "name": "tumor_bai",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.bai",
        "*.bam.bai"
      ],
      "help": ""
    },
    {
      "name": "normal_bam",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.bam"
      ],
      "help": ""
    },
    {
      "name": "normal_bai",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.bam.bai"
      ],
      "help": ""
    },
    {
      "name": "bed",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.bed"
      ],
      "help": "bed file for intervals"
    },
    {
      "name": "dockerimage_vardict",
      "class": "file",
      "optional": true,
      "default": {
        "$dnanexus_link": {
            "project": "project-G3Yj1vjJ6XG579jbKyjXPGGY",
            "id": "file-G4B0kG8J6XG6pf0j1gzz6bgV"
        }
      },
      "help": ""
    },
    {
      "name": "AF_THR",
      "label": "allel frequency threshold",
      "class": "string",
      "optional": true,
      "default": "0.005",
      "help": ""
    }
  ],
  "outputSpec": [
    {
      "name": "vcf",
      "class": "file",
      "patterns": [
        "*.vcf",
        "*.vcf.gz"
      ],
      "help": ""
    },
    {
      "name": "vcf_index",
      "class": "file",
      "patterns": [
        "*.vcf.gz.tbi"
      ],
      "help": ""
    }
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 2
      }
    },
    "interpreter": "bash",
    "file": "src/vardict.sh",
    "distribution": "Ubuntu",
    "release": "20.04",
    "version": "0"
  },
  "access": {
    "project": "CONTRIBUTE",
    "allProjects": "VIEW"
  },
  "regionalOptions": {
    "aws:eu-west-2": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem3_ssd1_v2_x16"
        }
      }
    }
  }
}
