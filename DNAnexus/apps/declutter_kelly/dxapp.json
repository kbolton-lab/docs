{
  "name": "declutter_kelly",
  "title": "declutter_kelly",
  "summary": "Currently just removing large BQSR bams after R annotation is complete",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "inputSpec": [
    {
      "name": "final_tsv",
      "label": "Final TSV",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.tsv"
      ],
      "help": ""
    },
    {
      "name": "project",
      "label": "project",
      "class": "string",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "bqsr_spark",
      "label": "bqsr_spark",
      "class": "string",
      "optional": true,
      "default": "bqsr_spark",
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "mutect",
      "label": "mutect",
      "class": "string",
      "optional": true,
      "default": "mutect_single",
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "vardict",
      "label": "vardict",
      "class": "string",
      "optional": true,
      "default": "vardict",
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "MT_vcf",
      "label": "MT_vcf",
      "class": "file",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": "making sure Mutect vcf completed before deleting BQSR bam"
    },
    {
      "name": "VD_vcf",
      "label": "VD_vcf",
      "class": "file",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": "making sure Vardict vcf completed before deleting BQSR bam"
    }
  ],
  "outputSpec": [
    {
      "name": "success_bam",
      "label": "success_bam",
      "class": "string",
      "patterns": [
        "*"
      ],
      "help": "print success"
    },
    {
      "name": "success_bai",
      "label": "success_bai",
      "class": "string",
      "patterns": [
        "*"
      ],
      "help": "print success"
    }
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 1
      }
    },
    "interpreter": "bash",
    "file": "src/declutter_kelly.sh",
    "distribution": "Ubuntu",
    "release": "20.04",
    "version": "0"
  },
  "access": {
    "project": "CONTRIBUTE"
  },
  "regionalOptions": {
    "aws:eu-west-2": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem1_ssd1_v2_x2"
        }
      }
    }
  }
}
