      "optional": true,
      "default": {
        "$dnanexus_link": {
          "project": "project-G3Yj1vjJ6XG579jbKyjXPGGY",
          "id": "file-G464PjQJ6XGK5YYg78FP1z1k"
        }
      }
    }
