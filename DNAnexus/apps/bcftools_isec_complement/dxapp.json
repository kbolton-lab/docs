{
  "name": "bcftools_isec_complement",
  "title": "VCF Complement",
  "summary": "Get complement of first file from second file",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "inputSpec": [
    {
      "name": "vcf_in",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.vcf*"
      ],
      "help": ""
    },
    {
      "name": "vcf_in_index",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.*cf*.tbi"
      ],
      "help": ""
    },
    {
      "name": "exclude_vcf",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.vcf*"
      ],
      "help": ""
    },
    {
      "name": "exclude_vcf_index",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.*cf*.tbi"
      ],
      "help": ""
    },
    {
      "name": "output_type",
      "class": "string",
      "optional": true,
      "default": "z",
      "help": ""
    },
    {
      "name": "caller",
      "class": "string",
      "optional": false,
      "help": ""
    },
    {
      "name": "output_vcf_suffix",
      "class": "string",
      "optional": true,
      "default": "bcftools_isec.vcf.gz",
      "help": ""
    }
  ],
  "outputSpec": [
    {
      "name": "complement_vcf",
      "class": "file",
      "patterns": [
        "*.vcf",
        "*.vcf.gz"
      ],
      "help": ""
    },
    {
      "name": "complement_vcf_index",
      "class": "file",
      "patterns": [
        "*.vcf.gz.tbi"
      ],
      "help": ""
    }
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 1
      }
    },
    "interpreter": "bash",
    "file": "src/bcftools_isec_complement.sh",
    "execDepends":[
      {"name": "bcftools"},
      {"name": "tabix"}
    ],
    "distribution": "Ubuntu",
    "release": "20.04",
    "version": "0"
  },
  "access": {
    "project": "CONTRIBUTE"
  },
  "regionalOptions": {
    "aws:eu-west-2": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem3_ssd1_v2_x2"
        }
      }
    }
  }
}
